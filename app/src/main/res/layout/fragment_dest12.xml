<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".navigation.Dest8LoginFragment">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mbtLoginSuccess1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="登录成功，直接返回" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mbtLoginCancel1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="可以不登录，pop当前目的地，返回首页继续使用" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mbtLoginExitApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="强制登录，直接返回退出App" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mbtLoginSuccess"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="强制登录，成功才能返回" />
        </androidx.appcompat.widget.LinearLayoutCompat>
    </FrameLayout>
</layout>